  <% if user_signed_in? %>
    <body class="fixed skin-blue">
      <%= render 'shared/alerts' %>
      <div class="wrapper">
        <%= render 'layouts/navigation/layout'%>
        <div class="content-wrapper">
          <%= yield %>
        </div>
        <%= render 'layouts/navigation/footer' %>
      </div>
      <div class="alerts"></div>
    </body>
  <% else %>
    <body class="hold-transition login-page">
      <%= render 'shared/alerts' %>
      <%= yield %>
