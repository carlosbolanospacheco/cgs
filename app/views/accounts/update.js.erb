<% if @cuenta.errors.size == 0 %>
  $(".canvas_cuenta").empty();
  $(".box_cuenta").addClass('hidden');
  $(".canvas_listado_cuentas").html("<%= escape_javascript(render 'accounts/listado') %>")
<% else %>
  $(".canvas_cuenta").html("<%= escape_javascript(render 'accounts/form') %>");
<% end %>
$(".alerts").html("<%= escape_javascript(render 'shared/alerts') %>")
