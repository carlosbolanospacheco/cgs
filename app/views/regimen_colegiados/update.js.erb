<% if @regimen.errors.size == 0 %>
  $(".canvas_regimen").empty();
  $(".box_regimen").addClass('hidden');
  $(".canvas_listado_regimenes").html("<%= escape_javascript(render 'regimen_colegiados/listado') %>")
<% else %>
  $(".canvas_regimen").html("<%= escape_javascript(render 'regimen_colegiados/form') %>");
<% end %>
$(".alerts").html("<%= escape_javascript(render 'shared/alerts') %>")
